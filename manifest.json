{
    "manifest_version": 3,
    "name": "Quickbit Calculator",
    "version": "1.0",
    "description": "A 3D dark-themed calculator for everyday purposes by Quickbit Solutions.",
    "action": {
      "default_popup": "popup.html",
      "default_icon":
      {
        "16": "calci16.png",
        "48": "calci48.png",
        "128": "calci128.png"
      }
    },
    "permissions": ["storage"],
     "icons": {
    "128": "calci128.png"
  }
}
  